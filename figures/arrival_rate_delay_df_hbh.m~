close all;
clear;
figure

p = 0.1;

lamda_1 = linspace(0, 0.9);
lamda_1 = linspace(0, 1.18);
lamda_1 = linspace(0, 0.9);

t_1 = 5 * (1-p)^5 ./ (2 * (1-p)^5 - lamda);
t_2 = (0.9^5 - lamda).^(-1) + (0.9^4 - lamda).^(-1) + (0.9^3 - lamda).^(-1) ...
    + (0.9^2 - lamda).^(-1) + (1.8 - lamda).^(-1);
t_3 = 0.9 ./ (1.8 - lamda) + 3.6 ./ (0.9 - lamda);

plot(lamda, t_1);
hold on
plot(lamda, t_2);
plot(lamda, t_3);
grid